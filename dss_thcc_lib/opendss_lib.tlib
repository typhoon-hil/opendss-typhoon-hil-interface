                                mdl.set_property_value(mdl.prop(item_handle, "Vreg"), Vreg)
                                mdl.set_tag_properties(FromVLDC, value="VLoad_ab", scope='local')
                                mdl.set_tag_properties(FromVLDC_unused, value="VLoad_a", scope='local')
                            else:
