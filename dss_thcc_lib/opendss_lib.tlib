                            maxtap = float(maxtap)
                            mintap = float(mintap)
                        
                            var_per_tap = (maxtap-mintap)/n_taps
                        
                            max_n_tap = int(round(((maxtap - 1) / (maxtap - mintap) * n_taps)))
                            min_n_tap = n_taps - max_n_tap
                        
                            tap_difference = max_n_tap - (n_taps // 2 + n_taps % 2)
                        
                            ptratio_auto = float(ptratio_auto)
